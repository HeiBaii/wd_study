var subFirst = function (val) {
  if (val.length === 0 || val === undefined) {
    return;
  }
  if (val.length > 5) {
    return val.substring(0, 5)
  } else {
    return val
  }
}

var subLast = function (val) {
  if (val.length === 0 || val === undefined) {
    return;
  }
  if (val.length > 5) {
    return val.substring(6, 11)
  } else {
    return val
  }
}

module.exports.subFirst = subFirst
module.exports.subLast = subLast